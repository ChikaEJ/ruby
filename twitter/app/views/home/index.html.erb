<h1>Main</h1>
<% @posts.each do |post| %>

  <div class="media">
    <div class="media-left media-middle">
      <a href="#">
        <img class="media-object" src="..." alt="...">
      </a>
    </div>
    <div class="media-body">
      <h4 class="media-heading"><%= link_to post.user.name, '#' %></h4>
      <%= post.text %>
    </div>
  </div>



<% end %>
