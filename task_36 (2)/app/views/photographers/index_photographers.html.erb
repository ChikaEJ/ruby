<h1>All photographers</h1>


  <table>
    <tr>
      <th>Name</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Phone</th>
    </tr>
    <% @photographers.each do |photographer| %>
    <tr>
      <td><%= link_to photographer.name, photographer_path(photographer) %></td>
      <td><%= link_to photographer.lastname, photographer_path(photographer) %></td>
      <td><%= photographer.email %></td>
      <td><%= photographer.phone %></td>
    </tr>
    <% end %>
  </table>
  <%= link_to 'Back', root_path %>
