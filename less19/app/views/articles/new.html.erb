<h1>Articles#new</h1>
<p>
</p>
<%= form_for :article, url: articles_create_path do |f| %>
<p>
  <%= link_to "All articles", articles_index_path %>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :content %><br>
  <%= f.text_area :content %>
</p>
<p>
  <%= f.label :author %><br>
  <%= f.text_field :author %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>
